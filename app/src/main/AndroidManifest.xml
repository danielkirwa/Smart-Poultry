<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.smartpoultry">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".AppSettings"
            android:exported="false" />
        <activity
            android:name=".FarmReduction"
            android:exported="false" />
        <activity
            android:name=".AppGuide"
            android:exported="false" />
        <activity
            android:name=".PoultryDevelopment"
            android:exported="false" />
        <activity
            android:name=".FeedFormulation"
            android:exported="false" />
        <activity
            android:name=".PoultryHousing"
            android:exported="false" />
        <activity
            android:name=".Dashboard"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".AuthActivity" />
        <activity android:name=".DiseaseMedication" />
        <activity android:name=".EggProductuon" />
        <activity android:name=".FarmRecords" />
        <activity android:name=".ChickManagement" />
        <activity android:name=".ChickenManagement" />
        <activity android:name=".VaccinationManagement" />
        <activity android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver
            android:name=".PoultryAlarm"
            android:enabled="true"
            android:exported="true" />
    </application>

</manifest>